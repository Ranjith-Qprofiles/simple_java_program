pipeline{
    agent any 
    stages{
        stage('Stage1') {
            steps {
                echo "Hello World"
                echo "First Line\nSecond Line\nThird line'n"
                echo "This is first stage"
            }
        }
        stage('Stage2') {
            steps {
                echo "This is second stage"
            }
        }
        stage('Stage3') {
            steps {
                echo "This is third stage"
            }
        }
    }
}
